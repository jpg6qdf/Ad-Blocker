{
    "name": "CS4501 AdBlocker Chrome Extension",
    "version": "1.0",
    "description": "CS4501 AdBlocker Chrome Extension",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "activeTab", "tabs", "storage"],
    "background": {
        "scripts": ["background.js"]
    },
    "manifest_version": 2,
    "browser_action": {
        "default_title": "AdBlock"
    }
}